//方阵转置 
#include<stdio.h>
int main(){
	for(i=0;i<m;i++){
	    for(j=0;j<n;j++){
		scanf("%d",&a[i][j]);
	    }
	}
	for ( i = 0; i < n; i++ ) {
        for ( j = 0; j < n; j++ ) {
            if ( i < j ){               //for和if  for(j=i;j<n;j++) 
                temp = a[i][j];
                a[i][j] = a[j][i];
                a[j][i] = temp;
            }
        }
    }
} 
